#include <iostream>
#include <string>
#include <cmath>

class Location {
public:
    Location(const std::string& _name, double _x, double _y) {
        this->name = _name;
        this->x = _x;
        this->y = _y;
    }

    double distanceTo(const Location& other) {
        double dx = other.x - x;
        double dy = other.y - y;
        return std::sqrt(dx * dx + dy * dy);
    }

    std::string name;
    double x;
    double y;
};

int main() {
    Location home("Home", 1.0, 2.0);
    Location school("School", 4.0, 6.0);

    std::cout << "Distance from " << home.name << " to " << school.name 
              << ": " << home.distanceTo(school) << std::endl;

    return 0;
}